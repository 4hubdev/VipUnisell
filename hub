    discord.TextSize = 11
    Instance.new("UICorner", discord).CornerRadius = UDim.new(0, 5)
    local discordStroke = Instance.new("UIStroke", discord)
    discordStroke.Color = Color3.fromRGB(138, 43, 226)
    discordStroke.Thickness = 2
    startPurpleGlow(discordStroke)
    discord.MouseButton1Click:Connect(function() setclipboard("https://discord.gg/invite") end)
    lbl(infoTab, "Version: 2.0.0 - VeykoHub", CONFIG.THEME.TextDim)
end

createGUI()

UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.T then
        teleportSequence()
    elseif input.KeyCode == Enum.KeyCode.Insert then
        local main = PlayerGui:FindFirstChild("VeykoHub_GUI"):FindFirstChild("MainFrame")
        if main then main.Visible = not main.Visible end
    elseif input.KeyCode == Enum.KeyCode.P then
        local gui = PlayerGui:FindFirstChild("VeykoHub_GUI")
        if gui then gui.Enabled = not gui.Enabled end
    end
end)
