        State.streamProofEnabled = not State.streamProofEnabled
        toggleStreamProof(State.streamProofEnabled)
        i.Text = State.streamProofEnabled and "‚óè" or "‚óã"
        i.TextColor3 = State.streamProofEnabled and CONFIG.THEME.Success or CONFIG.THEME.TextDim
    end)
    
    -- ESP Tab
    btn(espTab, "ESP", function(b, i)
        State.espEnabled = not State.espEnabled
        toggleESP(State.espEnabled)
        i.Text = State.espEnabled and "‚óè" or "‚óã"
        i.TextColor3 = State.espEnabled and CONFIG.THEME.Success or CONFIG.THEME.TextDim
    end)
    
    btn(espTab, "Wallhack", function(b, i)
        State.wallhackEnabled = not State.wallhackEnabled
        toggleWallhack(State.wallhackEnabled)
        i.Text = State.wallhackEnabled and "‚óè" or "‚óã"
        i.TextColor3 = State.wallhackEnabled and CONFIG.THEME.Success or CONFIG.THEME.TextDim
    end)
    
    -- Admin Tab
    if isAdmin() and adminTab then
        lbl(adminTab, "üëë Gestion Whitelist", CONFIG.THEME.Purple)
        
        local usernameFrame = Instance.new("Frame", adminTab)
        usernameFrame.Size = UDim2.new(1, 0, 0, 45)
        usernameFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        Instance.new("UICorner", usernameFrame).CornerRadius = UDim.new(0, 8)
        local inputStroke = Instance.new("UIStroke", usernameFrame)
        inputStroke.Color = Color3.fromRGB(180, 0, 0)
        inputStroke.Thickness = 1
        
        local usernameInput = Instance.new("TextBox", usernameFrame)
        usernameInput.Size = UDim2.new(1, -20, 1, -10)
        usernameInput.Position = UDim2.new(0, 10, 0, 5)
        usernameInput.BackgroundTransparency = 1
        usernameInput.Text = ""
        usernameInput.PlaceholderText = "Entrer un pseudo..."
        usernameInput.PlaceholderColor3 = Color3.fromRGB(120, 120, 120)
        usernameInput.TextColor3 = Color3.fromRGB(220, 20, 20)
        usernameInput.Font = Enum.Font.Gotham
        usernameInput.TextSize = 14
        usernameInput.TextXAlignment = Enum.TextXAlignment.Left
        usernameInput.ClearTextOnFocus = false
        
        local whitelistList
        local function updateWhitelistDisplay()
            for _, child in pairs(whitelistList:GetChildren()) do
                if child:IsA("Frame") then child:Destroy() end
            end
            
            for userId, data in pairs(State.whitelistedUsers) do
                local entry = Instance.new("Frame", whitelistList)
                entry.Size = UDim2.new(1, -10, 0, 30)
                entry.BackgroundColor3 = Color3.fromRGB(25, 5, 5)
                Instance.new("UICorner", entry).CornerRadius = UDim.new(0, 6)
                
                local crownIcon = Instance.new("TextLabel", entry)
                crownIcon.Size = UDim2.new(0, 25, 1, 0)
                crownIcon.BackgroundTransparency = 1
                crownIcon.Text = "üëë"
                crownIcon.TextSize = 16
                
                local nameLabel = Instance.new("TextLabel", entry)
                nameLabel.Size = UDim2.new(1, -60, 1, 0)
                nameLabel.Position = UDim2.new(0, 30, 0, 0)
                nameLabel.BackgroundTransparency = 1
                nameLabel.Text = data.Name
                nameLabel.TextColor3 = Color3.fromRGB(220, 20, 20)
                nameLabel.Font = Enum.Font.GothamBold
                nameLabel.TextSize = 12
                nameLabel.TextXAlignment = Enum.TextXAlignment.Left
                
                local removeBtn = Instance.new("TextButton", entry)
                removeBtn.Size = UDim2.new(0, 25, 0, 25)
                removeBtn.Position = UDim2.new(1, -28, 0.5, -12)
                removeBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
                removeBtn.Text = "‚úï"
                removeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
                removeBtn.Font = Enum.Font.GothamBold
                removeBtn.TextSize = 14
                Instance.new("UICorner", removeBtn).CornerRadius = UDim.new(1, 0)
                
                removeBtn.MouseButton1Click:Connect(function()
                    State.whitelistedUsers[userId] = nil
                    print("[ECLIPSE] üëë " .. data.Name .. " retir√© de la whitelist")
                    updateWhitelistDisplay()
                end)
            end
            
            local listLayout = whitelistList:FindFirstChildOfClass("UIListLayout")
            if listLayout then
                whitelistList.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y + 10)
            end
        end
        
        btn(adminTab, "üëë Whitelist", function(b, i)
            local username = usernameInput.Text:gsub("^%s*(.-)%s*$", "%1")
            
            if username == "" then
                i.Text = "‚úï"
                i.TextColor3 = CONFIG.THEME.Error
                b.Text = "üëë Pseudo vide!"
                task.wait(2)
                b.Text = "üëë Whitelist"
                i.Text = "‚óã"
                i.TextColor3 = CONFIG.THEME.TextDim
                return
            end
            
            local targetPlayer = nil
            for _, player in pairs(Players:GetPlayers()) do
                if player.Name:lower() == username:lower() then
                    targetPlayer = player
                    break
                end
            end
            
            if not targetPlayer then
                i.Text = "‚úï"
                i.TextColor3 = CONFIG.THEME.Error
                b.Text = "üëë Joueur introuvable!"
                task.wait(2)
                b.Text = "üëë Whitelist"
                i.Text = "‚óã"
                i.TextColor3 = CONFIG.THEME.TextDim
                return
            end
            
            if not State.whitelistedUsers[targetPlayer.UserId] then
                State.whitelistedUsers[targetPlayer.UserId] = {
                    Name = targetPlayer.Name,
                    Time = os.date("%H:%M:%S")
                }
                i.Text = "‚úì"
                i.TextColor3 = CONFIG.THEME.Success
                b.Text = "üëë " .. targetPlayer.Name .. " whitelist√©!"
                print("[ECLIPSE] üëë " .. targetPlayer.Name .. " whitelist√©!")
                updateWhitelistDisplay()
                task.wait(2)
                b.Text = "üëë Whitelist"
                i.Text = "‚óè"
                i.TextColor3 = CONFIG.THEME.Success
                usernameInput.Text = ""
            else
                i.Text = "‚ö†"
                i.TextColor3 = CONFIG.THEME.Yellow
                b.Text = "üëë D√©j√† whitelist√©!"
                task.wait(2)
                b.Text = "üëë Whitelist"
                i.Text = "‚óè"
                i.TextColor3 = CONFIG.THEME.Success
            end
        end)
        
        lbl(adminTab, "üìã Joueurs Whitelist√©s:", CONFIG.THEME.Accent)
        
        whitelistList = Instance.new("ScrollingFrame", adminTab)
        whitelistList.Size = UDim2.new(1, 0, 0, 120)
        whitelistList.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
        whitelistList.BorderSizePixel = 0
        whitelistList.ScrollBarThickness = 3
        whitelistList.ScrollBarImageColor3 = CONFIG.THEME.Accent
        whitelistList.CanvasSize = UDim2.new(0, 0, 0, 0)
        Instance.new("UICorner", whitelistList).CornerRadius = UDim.new(0, 8)
        local listStroke = Instance.new("UIStroke", whitelistList)
        listStroke.Color = Color3.fromRGB(180, 0, 0)
        listStroke.Thickness = 1
        
        local listLayout = Instance.new("UIListLayout", whitelistList)
        listLayout.Padding = UDim.new(0, 5)
        listLayout.SortOrder = Enum.SortOrder.LayoutOrder
        
        local listPadding = Instance.new("UIPadding", whitelistList)
        listPadding.PaddingTop = UDim.new(0, 5)
        listPadding.PaddingLeft = UDim.new(0, 5)
        listPadding.PaddingRight = UDim.new(0, 5)
        listPadding.PaddingBottom = UDim.new(0, 5)
        
        updateWhitelistDisplay()
    end
    
    -- Premium Tab
    lbl(premTab, "‚ú® Scripts Premium", CONFIG.THEME.Purple)
    for _, s in ipairs(CONFIG.PREMIUM_SCRIPTS) do
        btn(premTab, s.emoji .. " " .. s.name, function(b, i) loadPremiumScript(s, b, i) end)
    end
    
    -- Key Tab
    lbl(keyTab, "üéÆ Raccourcis Clavier:", CONFIG.THEME.Accent)
    lbl(keyTab, "[T] - T√©l√©porter", CONFIG.THEME.TextDark)
    lbl(keyTab, "[Q] - Speed Boost ON/OFF", CONFIG.THEME.TextDark)
    lbl(keyTab, "[INSERT] - Ouvrir/Fermer Menu", CONFIG.THEME.TextDark)
    lbl(keyTab, "[P] - Stream Proof", CONFIG.THEME.TextDark)
    
    -- Info Tab
    lbl(infoTab, "üï∑Ô∏è Discord:", CONFIG.THEME.Accent)
    local discord = Instance.new("TextButton", infoTab)
    discord.Size = UDim2.new(1, 0, 0, 40)
    discord.BackgroundColor3 = Color3.fromRGB(220, 20, 20)
    discord.Text = "https://discord.gg/unisell"
    discord.TextColor3 = Color3.fromRGB(255, 255, 255)
    discord.Font = Enum.Font.GothamBold
    discord.TextSize = 12
    Instance.new("UICorner", discord).CornerRadius = UDim.new(0, 8)
    local discordStroke = Instance.new("UIStroke", discord)
    discordStroke.Color = Color3.fromRGB(180, 0, 0)
    discordStroke.Thickness = 2
    discord.MouseButton1Click:Connect(function() setclipboard("https://discord.gg/unisell") end)
    lbl(infoTab, "Version: 2.1.0 Shoni Fusion üî¥", CONFIG.THEME.TextDim)
    lbl(infoTab, "Cr√©√© par 8v & 6v & 4v", CONFIG.THEME.TextDim)
    
    -- Activer Misc par d√©faut
    tabs["Misc"].Content.Visible = true
    tabs["Misc"].Button.BackgroundColor3 = Color3.fromRGB(220, 20, 20)
    tabs["Misc"].Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    createFPSCounter()
end

createGUI()

-- ============================================================
-- KEYBINDS
-- ============================================================
UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.T then
        -- T√©l√©portation
        teleportSequence()
    elseif input.KeyCode == Enum.KeyCode.Q then
        -- Speed Boost ON/OFF (touche Q sur AZERTY = A sur QWERTY)
        State.speedBoostActive = not State.speedBoostActive
        toggleSpeedBoost(State.speedBoostActive)
        
        -- Notification visuelle
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "‚ö° Speed Boost";
            Text = State.speedBoostActive and "Activ√© ! (28 WalkSpeed)" or "D√©sactiv√©";
            Duration = 2;
        })
    elseif input.KeyCode == Enum.KeyCode.Insert then
        -- Ouvrir/Fermer le menu
        local gui = PlayerGui:FindFirstChild("ECLIPSE_GUI")
        if gui then
            local main = gui:FindFirstChild("Frame")
            local reopen = gui:FindFirstChild("TextButton")
            if main and reopen then
                State.guiVisible = not State.guiVisible
                main.Visible = State.guiVisible
                reopen.Visible = not State.guiVisible
            end
        end
    elseif input.KeyCode == Enum.KeyCode.P then
        -- Stream Proof
        local gui = PlayerGui:FindFirstChild("ECLIPSE_GUI")
        if gui then gui.Enabled = not gui.Enabled end
    end
end)

-- Respawn handler pour Speed Boost (maintient l'√©tat apr√®s respawn)
LocalPlayer.CharacterAdded:Connect(function(newChar)
    task.wait(1)
    if State.speedBoostActive then
        -- R√©activer le speed boost avec la nouvelle protection
        State.speedBoostActive = false
        task.wait(0.1)
        State.speedBoostActive = true
        toggleSpeedBoost(true)
    end
end)
