    local function createTab(name, shortName)
        local tabBtn = Instance.new("TextButton", tabBar)
        tabBtn.Size = UDim2.new(0, 52, 1, 0)
        tabBtn.BackgroundColor3 = Color3.fromRGB(5, 0, 10)
        tabBtn.Text = shortName or name
        tabBtn.TextColor3 = Color3.fromRGB(120, 80, 150)
        tabBtn.Font = Enum.Font.GothamBold
        tabBtn.TextSize = 10
        Instance.new("UICorner", tabBtn).CornerRadius = UDim.new(0, 5)
        local tabStroke = Instance.new("UIStroke", tabBtn)
        tabStroke.Color = Color3.fromRGB(50, 20, 70)
        tabStroke.Thickness = 1
        
        local container = Instance.new("Frame", content)
        container.Size = UDim2.new(1, -5, 0, 0)
        container.BackgroundTransparency = 1
        container.Visible = false
        local layout = Instance.new("UIListLayout", container)
        layout.Padding = UDim.new(0, 6)
        
