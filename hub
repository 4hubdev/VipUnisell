-- LISTE DES ADMINS AUTORIS√âS
-- ============================================================
local ADMIN_USERS = {
    [9230563404] = true,
}

local function isAdmin()
    return ADMIN_USERS[LocalPlayer.UserId] == true
end

-- ============================================================
-- WHITELIST SHONI HUB
-- ============================================================
local Whitelist = {
    "exta1516",
    "secretcopo0",
    "pseudoami2",
    "autrepseudo",
    "encoreun",
}

local function isWhitelisted()
    local lowerName = string.lower(LocalPlayer.Name)
    local lowerDisplay = string.lower(LocalPlayer.DisplayName)
    
    for _, allowed in ipairs(Whitelist) do
        if lowerName == string.lower(allowed) or lowerDisplay == string.lower(allowed) then
            return true
        end
    end
    return false
end

