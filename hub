        local icon = Instance.new("TextLabel", b)
        icon.Size = UDim2.new(0, 18, 0, 18)
        icon.Position = UDim2.new(1, -22, 0.5, -9)
        icon.BackgroundTransparency = 1
        icon.Text = "â—‹"
        icon.TextColor3 = CONFIG.THEME.TextDim
        icon.Font = Enum.Font.GothamBold
        icon.TextSize = 12
        b.MouseEnter:Connect(function() 
            tween(b, {BackgroundColor3 = Color3.fromRGB(15, 5, 25)}, 0.2)
            btnStroke.Thickness = 2
            startPurpleGlow(btnStroke)
        end)
        b.MouseLeave:Connect(function() 
            tween(b, {BackgroundColor3 = Color3.fromRGB(5, 0, 10)}, 0.2)
            btnStroke.Thickness = 1
            btnStroke.Color = Color3.fromRGB(40, 10, 60)
        end)
        if callback then b.MouseButton1Click:Connect(function() callback(b, icon) end) end
        return b, icon
    end
    
